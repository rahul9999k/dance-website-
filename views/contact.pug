extends base.pug

block scripts
  script(src='/static/index.js')

block style
    style
      include  ../static/style.css
      include ../static/stylecontact.css
block content
    div.container
      h1 Contact Us
      form(action="/contact",method='post',class='myform') 
          input(type="text", class='myinput',name='Name', placeholder='enter your Name')
          input(type="number", class='myinput',name='number', placeholder='enter your phone')
          input(type="text", class='myinput',name='email', placeholder='enter your e-mail')
          input(type="text", class='myinput',name='address', placeholder='enter your address')
          input(type="date", class='myinput',name='AGE', placeholder='how old are you')
          button(class='btn') Submit
            